{
	"name": "vscode-twig",
	"version": "0.1.0",
	"publisher": "SiliconSoldier",
	"author": {
		"name": "Jordan Mele",
		"email": "SiliconSoldier@outlook.com.au"
	},
	"contributors": [
		{
			"name": "William Holroyd",
			"role": "Creator of https://github.com/wholroyd/vscode-jinja, of which this extension is based."
		}
	],
	"license": "MIT",
	"icon": "icon.png",
	"description": "Twig template language support for Visual Studio Code",
	"displayName": "Twig Templating",
	"repository": {
		"type": "git",
		"url": "https://github.com/Silic0nS0ldier/vscode-twig.git"
	},
	"bugs": {
		"url": "https://github.com/Silic0nS0ldier/vscode-twig/issues"
	},
	"categories": [
		"Languages",
		"Snippets",
		"Linters"
	],
	"engines": {
		"vscode": "^1.0.0"
	},
	"contributes": {
		"languages": [
			{
				"id": "twig",
				"aliases": [
					"Twig"
				],
                "extensions": [".twig"],
				"configuration": "./twig.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "twig",
				"scopeName": "source.twig",
				"path": "./syntaxes/twig.json"
			},
			{
				"language": "html.twig",
				"scopeName": "text.html.twig",
				"path": "./syntaxes/twig-html.json"
			},
			{
				"language": "yaml.twig",
				"scopeName": "text.yaml.twig",
				"path": "./syntaxes/twig-yaml.json"
			}
		]
	}
}